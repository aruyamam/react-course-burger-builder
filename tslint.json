{
   "extends": [
      "tslint:latest",
      "tslint-react",
      "tslint-config-airbnb",
      "tslint-eslint-rules",
      "tslint-config-prettier"
   ],
   "rules": {
      "import-name": false,
      "indent": [true, "spaces", 3],
      "jsx-no-multiline-js": false,
      "no-debugger": false,
      "no-console": false,
      "object-literal-sort-keys": false,
      "ordered-imports": false,
      "ter-indent": [true, 3]
   },
   "linterOptions": {
      "exclude": [
         "config/**/*.js",
         "node_modules/**/*.ts",
         "coverage/lcov-report/*.js"
      ]
   }
}
